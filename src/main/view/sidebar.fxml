<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="150.0" style="-fx-background-color: #4059a9;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.SidebarController">
   <children>
      <JFXButton fx:id="projectButton" layoutX="35.0" layoutY="90.0" onAction="#displayProject" prefHeight="30.0" prefWidth="80.0" style="-fx-border-color: white;" text="Project" textFill="WHITE" />
      <JFXButton fx:id="calendarButton" layoutX="35.0" layoutY="240.0" onAction="#displayCalendar" prefHeight="30.0" prefWidth="80.0" style="-fx-border-color: white;" text="Calendar" textFill="WHITE" />
      <JFXButton fx:id="logout" layoutX="35.0" layoutY="550.0" onAction="#logoutUser" prefHeight="30.0" prefWidth="80.0" style="-fx-border-color: white;" text="Logout" textFill="WHITE" />
      <JFXButton fx:id="employeeButton" layoutX="35.0" layoutY="190.0" onAction="#displayEmployee" prefHeight="30.0" prefWidth="80.0" style="-fx-border-color: white;" text="Employee" textFill="WHITE" />
      <Label layoutX="30.0" layoutY="20.0" text="Dashboard" textFill="WHITE">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <JFXButton fx:id="activityButton" layoutX="36.0" layoutY="140.0" onAction="#displayActivity" prefHeight="30.0" prefWidth="80.0" style="-fx-border-color: white;" text="Activity" textFill="WHITE" />
   </children>
</AnchorPane>
